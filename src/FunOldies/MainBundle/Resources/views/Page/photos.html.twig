{% extends 'FunOldiesMainBundle::layout.html.twig' %}

{% block title %}
    Photos - Fun Oldies  - Prescott Radio
{% endblock %}

{% block description %}
    <meta name="description" content="Photos - Fun Oldies - Prescott"/>
    <meta property="og:site_name" content="http://funoldies.gcmaz.com" />
    <meta property="fb:admins" content="100003228578475" />
    <meta property="fb:app_id" content="479469978793919" />
    <meta property="og:type" content="music.radio_station" />   
    <meta property="og:image" content="http://funoldies.gcmaz.com{{ asset('images/logo-oldies.png') }}"/>
    <meta property="og:url" content="http://funoldies.gcmaz.com{{ path('fo_photos') }}"/>
    <meta property="og:title" content="Photos - Fun Oldies - Prescott"/> 
    <meta property="og:description" content="Photos - Fun Oldies - Prescott"/>
{% endblock %}

{% block keywords %}
    <meta name="keywords" content="Photos, Prescott, Northern Arizona"/>
{% endblock %}
    
{% block styles %}
    {{ parent() }}
    <style type="text/css">
    .photos a{ text-decoration:none; font-weight:700; color:#21656d; font-size:1em;line-height:2; }
    .photos-thumb{ padding:10px; }
    </style>
{% endblock %}
 
{% block js_top %}
    {{ parent() }}
    <script type="text/javascript" src="{{ asset('scripts/galleria/galleria-1.2.9.js') }}"></script>
    <script type="text/javascript" src="{{ asset('scripts/galleria/themes/classic/galleria.classic.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('scripts/galleria/plugins/facebook/galleria.facebook.js') }}"></script>
{% endblock %}
    
{% block content %}
    
    <div class="photos">
        <h2 class="cinzel">Photos</h2>
        <!doctype html>
        <html>
        <body>
        {{ display_block|raw  }}
        </body>
        </html>
    </div>
    <br class="clear"/>
    <div class="fb-like" data-href="http://funoldies.gcmaz.com{{ path('fo_photos') }}" data-send="true" data-width="450" data-show-faces="true" data-font="arial"></div>
    <br class="clear"/><br/>
{% endblock %}